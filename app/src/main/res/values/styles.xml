<resources>

    <style name="AppTheme" parent="android:Theme.Light" />

    <style name="PreyTheme" parent="@android:style/Theme.Black">
        <item name="android:background">@null</item>
    </style>

    <style name="Theme.Base" parent="Theme.AppCompat.Light">
    </style>

    <style name="PreyTextAppearance" parent="@android:style/TextAppearance">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#333</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="PreyTextAppearance2" parent="@android:style/TextAppearance">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="PreyButton">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">22dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:background">@drawable/button_bg_selector</item>

    </style>

    <style name="PreyButton2">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginRight">40dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:background">@drawable/button_round</item>
        <item name="android:gravity">center</item>
    </style>



    <style name="PreyButton2.Large">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_marginLeft">70dp</item>
        <item name="android:layout_marginRight">70dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:background">@drawable/button_round</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="PreyButton2.Small">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/button_round</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="PreyButton3">
        <item name="android:textColor">#39A6C6</item>
        <item name="android:textSize">14sp</item>

        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_marginTop">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginRight">40dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/button_round</item>
    </style>

    <style name="PreyButton3.Large">
        <item name="android:textColor">#047298</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_marginLeft">70dp</item>
        <item name="android:layout_marginRight">70dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/button_round</item>
    </style>

    <style name="PreyButton3.Small">
        <item name="android:textColor">#047298</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/button_round</item>
    </style>
    <style name="PreyButton4">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:background">@drawable/button_round</item>
    </style>

    <style name="PreyButton.Large">
        <item name="android:textSize">30sp</item>
        <item name="android:layout_height">72dp</item>
    </style>

    <style name="PreyButton.XLarge">
        <item name="android:textSize">45sp</item>
        <item name="android:layout_height">96dp</item>
    </style>

    <style name="PreyButton.Camouflage">
        <item name="android:background">@drawable/button_camouflage_selector</item>
    </style>

    <style name="PreyButton.Large.Camouflage">
        <item name="android:background">@drawable/button_camouflage_selector</item>
    </style>

    <style name="PreyButton.XLarge.Camouflage">
        <item name="android:background">@drawable/button_camouflage_selector</item>
    </style>

    <style name="PreyEditText">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:typeface">normal</item>
        <item name="android:background">@drawable/input_text2</item>
        <item name="android:ems">8</item>
    </style>


    <style name="PreyEditText.Large">
        <item name="android:layout_height">72dp</item>
        <item name="android:textSize">25sp</item>
        <item name="android:layout_marginLeft">50dp</item>
        <item name="android:layout_marginRight">50dp</item>
    </style>

    <style name="PreyEditText.XLarge">
        <item name="android:layout_height">96dp</item>
        <item name="android:textSize">33sp</item>
        <item name="android:layout_marginLeft">50dp</item>
        <item name="android:layout_marginRight">50dp</item>
    </style>

    <style name="Theme" parent="android:Theme.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="Theme.Transparent" parent="android:Theme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="Theme.Preference" parent="android:Theme">
        <item name="android:textColor">#000000</item>
        <item name="android:cacheColorHint">@color/black</item>
        <item name="android:background">@color/white</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="android:colorBackground">@color/white</item>
        <item name="android:listViewStyle">@style/listViewPrefs</item>
    </style>

    <style name="listViewPrefs" parent="@android:style/Widget.ListView">
        <item name="android:background">@color/white</item>
        <item name="android:cacheColorHint">@color/black</item>
        <item name="android:textColor">@color/black</item>
    </style>

</resources>
